{
  "manifest_version": 3,
  "name": "CER Sort Security Alerts",
  "version": "1.0",
  "description": "CER - sort available and selected onsite alert ids",

  "icons": {
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "content_scripts": [
    {
      "js": ["scripts/content.js"],
      "matches": [
        "https://*/ceradmin/servlet/CERAdminServlet?formname=zoneconfig*"
      ]
    }
  ]
}